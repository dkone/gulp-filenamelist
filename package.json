{
    "name": "gulp-filenamelist",
    "version": "1.0.2",
    "description": "Creates a file containing a list of filenames",
    "homepage": "https://github.com/pindab0ter/gulp-filenamelist#readme",
    "author": "Hans van Luttikhuizen <hansvanluttikhuizen@me.com>",
    "license": "MIT",
    "keywords": [
        "gulpplugin"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pindab0ter/gulp-filenamelist.git"
    },
    "bugs": {
        "url": "https://github.com/pindab0ter/gulp-filenamelist/issues"
    },
    "main": "main.js",
    "scripts": {
        "test": "npm run sonarreport && npm run xunitreport",
        "sonarreport": "mocha -R mocha-sonar-generic-test-coverage test > reports/tests-sonar.xml",
        "xunitreport": "mocha -R xunit test > reports/tests-xunit.xml",
        "pretest": "npm run coverage",
        "coverage": "istanbul cover node_modules/mocha/bin/_mocha"
    },
    "engines": {
        "node": ">= 0.10"
    },
    "dependencies": {
        "gulp-util": "^3.0.7",
        "through2": "^2.0.1"
    },
    "devDependencies": {
        "gulp": "^3.9.1",
        "istanbul": "^0.4.2",
        "mocha": "^2.4.5",
        "mocha-sonar-generic-test-coverage": "0.0.1"
    }
}
